<% content_for :head do %>
  <%= render 'layouts/stylesheets' %>
<% end %>
<div class="container">
  <section class="round">
    <h2>รายชื่อผู้ใช้</h2>
    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= gravatar_for user, :size => 40 %>
          <%= link_to user.user_name, user %>
        </li>
      <% end %>
    </ul>
  </section>
</div>
